{
  "name": "eduvision-app",
  "version": "1.0.0",
  "description": "AI-powered interactive learning platform that transforms study materials into engaging presentations with voice narration and quizzes",
  "keywords": [
    "education",
    "ai",
    "interactive-learning",
    "presentation",
    "quiz",
    "voice-narration",
    "study-tool"
  ],
  "author": "EduVision Team",
  "license": "MIT",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "lucide-react": "0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
